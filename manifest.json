{
  "name": "NetMonitor",
  "version": "1.1",
  "description": "NetMonitor it run periodically and check your bandwidth is higher than 200 mbps or not",
  "author": "Navjot Tomer",
  "icons": {
    "128": "icons/logo.png"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/logo.png",
    "default_title": "NetMonitor",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "*://*.digitalocean.com/*"
  ]
}